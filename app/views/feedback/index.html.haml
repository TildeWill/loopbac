= simple_form_for(@feedback) do |f|
  = f.input :about_email, as: :contact_picker, input_html: {data: {contacts: "[#{current_users.map(&:to_autocomplete_json).join(",")}]"}}
  = f.input :details
  = f.submit

- @feedback_given.each do |feedback|
  .feedback-given
    = feedback.about_email
    = feedback.details

- @feedback_received.each do |feedback|
  .feedback-received
    = feedback.user.name
    = feedback.details
